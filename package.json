{
  "name": "webpack-example",
  "version": "1.0.0",
  "description": "webpack-example",
  "scripts": {
    "build": "cross-env NODE_ENV=BUILD webpack --progress --display-chunks --display-reasons --display-error-details --display-modules",
    "dist": "cross-env NODE_ENV=DIST webpack --progress --display-chunks --display-reasons --display-error-details --display-modules",
    "watch": "npm run build -- --watch",
    "prestart": "npm install",
    "start": "cross-env NODE_ENV=DEV  webpack-dashboard -- webpack-dev-server --config ./webpack.config.js",
    "test:watch": "karma start karma.conf.js --singleRun=false",
    "test:once": "karma start karma.conf.js --singleRun=true"
  },
  "keywords": ["webpack2","es6","angular","karma","scss","sass"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-istanbul-instrumenter-loader": "^1.0.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-growl-reporter": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-webpack": "^2.0.3",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "open-browser-webpack-plugin": "0.0.5",
    "sass-loader": "^6.0.5",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dashboard": "^0.4.0"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-route": "^1.6.4",
    "moment": "^2.18.1"
  }
}
